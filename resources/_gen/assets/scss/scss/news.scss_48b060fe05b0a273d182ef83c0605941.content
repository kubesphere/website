.section-1{position:relative;padding-top:138px;padding-bottom:100px}.section-1 .title-div{text-align:center}.section-1 p{margin:12px auto 0;letter-spacing:-.04px;color:#fff}.section-2{padding-top:60px;padding-bottom:70px;background-color:#fff}.section-2 .news-div{position:relative;padding:32px;padding-right:274px;margin-bottom:12px;border-radius:4px;border:solid 1px rgba(145,154,163,.6)}@media only screen and (max-width:768px){.section-2 .news-div{padding:10px}}.section-2 .news-div h2 a{font-size:20px;font-weight:600;line-height:1.6;color:#36435c}.section-2 .news-div h2 a:hover{color:#55bc8a}.section-2 .news-div p{margin-top:12px;font-size:16px;line-height:2;color:#919aa3}.section-2 .news-div img{position:absolute;top:50%;right:32px;transform:translateY(-50%);max-width:158px}@media only screen and (max-width:768px){.section-2 .news-div img{display:none}}.section-2 #pagination{margin-top:40px;text-align:center}.section-2 #pagination li{display:inline-block;margin:0 10px}.section-2 #pagination .disabled a{color:#ccd3db}.section-2 #pagination .active a{color:#55bc8a}