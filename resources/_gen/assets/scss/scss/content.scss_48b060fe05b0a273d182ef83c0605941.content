.navigation{box-shadow:0 4px 8px 0 rgba(36,46,66,.06),0 8px 16px 0 rgba(36,46,66,.05);background-image:linear-gradient(to bottom,rgba(134,219,162,0.9),rgba(0,170,114,0.9))}.main-section>div{position:relative;padding-top:93px}.main-section>div .breadcrumb{margin:30px 0}.main-section>div .breadcrumb a{color:#919aa3}.main-section>div .breadcrumb a:hover{color:#55bc8a}.main-section>div .main-div{box-sizing:border-box;width:880px;padding:40px 60px;background-color:#fff;border-radius:4px}@media only screen and (max-width:768px){.main-section>div .main-div{width:100%;padding:0;background-color:#f5f8f9}}.main-section>div .main-div .author{font-size:16px;line-height:1.5;color:#171c34}.main-section>div .main-div .date{font-size:14px;line-height:1.43;color:#919aa3}.main-section>div .main-div h1{margin-top:20px;margin-bottom:40px;text-shadow:0 8px 16px rgba(35,45,65,.1);font-size:40px;font-weight:500;line-height:1.4;color:#171c34}@media only screen and (max-width:768px){.main-section>div .main-div h1{font-size:28px}}.main-section>div .main-div .share-1{margin-bottom:20px}.main-section>div .main-div .share-2{margin-top:20px}.main-section>div .main-div .content .md-body{font-size:16px;line-height:2.29;color:#36435c}.main-section>div .main-div .content .md-body h2{font-weight:500;line-height:64px;color:#171c34;text-shadow:none;text-align:left;margin-bottom:20px;border-bottom:1px solid #ccd3db}.main-section>div .main-div .content .md-body h3{font-weight:600;line-height:1.5;color:#171c34}.main-section>div .main-div .content .md-body img{max-width:100%;box-sizing:content-box;background-color:#fff;border-radius:5px;box-shadow:none}.main-section>div .main-div .content .md-body blockquote{padding:4px 20px 4px 12px;border-radius:4px;background-color:#ecf0f2}.main-section>div .main-div .content-metadata{margin-bottom:28px}.main-section>div .main-div .content-metadata-title{font-size:16px;font-weight:500;line-height:1.5;color:#171c34}.main-section>div .main-div .content-metadata-time{font-